{
  "name": "electron-main",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "dist/index.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js",
    "lint": "eslint . --ext js,ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron ../../scripts/update-electron-vendors.js"

  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "8.0.1",
    "cross-env": "7.0.3",
    "electron": "31.3.1",
    "electron-builder": "24.13.3",
    "eslint": "9.8.0",
    "typescript": "5.5.4",
    "vite": "5.3.5"
  },
  "dependencies": {
    "electron-updater": "6.2.1"
  }
}
